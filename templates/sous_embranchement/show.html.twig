{% extends 'base.html.twig' %}

{% block title %}SousEmbranchement{% endblock %}

{% block body %}
    <h1>SousEmbranchement</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ sous_embranchement.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ sous_embranchement.nom }}</td>
            </tr>
            <tr>
                <th>Vernaculaire</th>
                <td>{{ sous_embranchement.vernaculaire }}</td>
            </tr>
        </tbody>
    </table>

     <table class="table">
        <tbody>
        {% for superClasse in sous_embranchement.superClasses %}
            
            <tr>

                <td>   <a href="{{ path('super_classe_show', {'id': superClasse.id}) }}"><h2>{{ superClasse.nom }}</h2></a></td>
            </tr>
            {% endfor %}
        </tbody>
    </table>

     <table class="table">
        <tbody>
        {% for Classe in sous_embranchement.Classes %}
            
            <tr>

                <td>   <a href="{{ path('super_classe_show', {'id': Classe.id}) }}"><h2>{{ Classe.nom }}</h2></a></td>
            </tr>
            {% endfor %}
        </tbody>
    </table>


    <a href="{{ path('sous_embranchement_index') }}">back to list</a>

    <a href="{{ path('sous_embranchement_edit', {'id': sous_embranchement.id}) }}">edit</a>

    {{ include('sous_embranchement/_delete_form.html.twig') }}
{% endblock %}
