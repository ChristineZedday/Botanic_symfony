{% extends 'base.html.twig' %}

{% block title %}Embranchements{% endblock %}

{% block content %}
    <h1>Embranchements</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Nom</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for embranchement in embranchements %}
            <tr>
                <td>{{ embranchement.id }}</td>
                <td>{{ embranchement.nom }}</td>
                <td>
                    <a href="{{ path('embranchement_show', {'id': embranchement.id}) }}">show</a>
                    <a href="{{ path('embranchement_edit', {'id': embranchement.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('embranchement_new') }}">Create new</a>
{% endblock %}
